# Source files common to all targets
SRC = encoder.c i2c.c lsm303agr.c motor.c radio.c global.c

# Object files generated from the source files
OBJ = $(subst .c,.o,$(SRC))
LIB=libbsp2.a

CFLAGS += -I ../bsp

include ../rules.mk

# The following lines can be auto-generated by make depend
encoder.o: encoder.c encoder.h global.h
i2c.o: i2c.c i2c.h
lsm303agr.o: lsm303agr.c lsm303agr.h i2c.h
motor.o: motor.c nrf52833.h motor.h
radio.o: radio.c radio.h
global.o: global.c global.h
