# Source files common to all targets
SRC = gpio.c ledbtn.c uart.c printf.c pwm.c adc.c audio.c bsp.c

# Object files generated from the source files
OBJ = $(subst .c,.o,$(SRC))
LIB=libbsp.a

include ../rules.mk

# The following lines can be auto-generated by make depend
gpio.o: gpio.c gpio.h
ledbtn.o: ledbtn.c ledbtn.h microbit.h gpio.h
uart.o: uart.c gpio.h uart.h
printf.o: printf.c printf.h
pwm.o: pwm.c pwm.h
adc.o: adc.c adc.h
audio.o: audio.c audio.h gpio.h pwm.h adc.h
bsp.o: bsp.c bsp.h microbit.h gpio.h ledbtn.h uart.h pwm.h adc.h audio.h \
 printf.h
